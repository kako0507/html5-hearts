{
  "extends": "airbnb",
  "settings": {
    "react": {
      "pragma": "React",
      "version": "16.3"
    }
  },
  "parser": "babel-eslint",
  "plugins": [
    "react"
  ],
  "globals": {
    "document": false,
    "window": false
  },
  "rules": {
    "global-require": 1,
    "jsx-a11y/click-events-have-key-events": 1,
    "react/forbid-prop-types": 1,
    "react/require-default-props": 1,
  }
}
